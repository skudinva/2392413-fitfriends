{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@backend/account-notify": ["libs/account/notify/src/index.ts"],
      "@backend/authentication": ["libs/account/authentication/src/index.ts"],
      "@backend/config": ["libs/account/config/src/index.ts"],
      "@backend/data-access": ["libs/shared/data-access/src/index.ts"],
      "@backend/email-subscriber": [
        "libs/notify/email-subscriber/src/index.ts"
      ],
      "@backend/file-uploader": ["libs/file-vault/file-uploader/src/index.ts"],
      "@backend/file-vault-config": ["libs/file-vault/config/src/index.ts"],
      "@backend/friend": ["libs/account/friend/src/index.ts"],
      "@backend/helpers": ["libs/shared/helpers/src/index.ts"],
      "@backend/interceptors": ["libs/shared/interceptors/src/index.ts"],
      "@backend/models": ["libs/trainings/models/src/index.ts"],
      "@backend/notify-config": ["libs/notify/config/src/index.ts"],
      "@backend/pipes": ["libs/shared/pipes/src/index.ts"],
      "@backend/shared/core": ["libs/shared/core/src/index.ts"],
      "@backend/shop-user": ["libs/account/shop-user/src/index.ts"],
      "@backend/training": ["libs/trainings/training/src/index.ts"],
      "@backend/training-comment": [
        "libs/trainings/training-comment/src/index.ts"
      ],
      "@backend/training-config": [
        "libs/trainings/training-config/src/index.ts"
      ],
      "@backend/training-models": ["libs/trainings/models/src/index.ts"],
      "@backend/training-notify": [
        "libs/trainings/training-notify/src/index.ts"
      ],
      "@backend/training-order": ["libs/trainings/training-order/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
